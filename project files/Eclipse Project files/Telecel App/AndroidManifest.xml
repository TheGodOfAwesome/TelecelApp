<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.telecelapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="11" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.telecelapp.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.example.telecelapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.telecelapp.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.example.telecelapp.Recharge"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.telecelapp.Recharge" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.telecelapp.Call_Me_Back"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.telecelapp.Call_Me_Back" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.telecelapp.Promotions"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.telecelapp.Promotions" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="CameraTestActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.telecelapp.CameraTestActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="VideoViewConsole"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.telecelapp.VideoViewConsole" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
